{
  "ConnectionStrings": {
    "NAuthContext": "${ConnectionStrings__NAuthContext}"
  },
  "NAuth": {
    "BucketName": "Emagine",
    "JwtSecret": "${NAuth__JwtSecret}"
  },
  "NTools": {
    "ApiURL": "http://ntools-api:80"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "NAuth": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "LogLevel": {
        "Default": "Information",
        "NAuth": "Information"
      }
    }
  }
}